<templateSet group="Python">
  <template name="type" value="type($SELECTION$)" description="Type(***)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
      <option name="Python_Class" value="false" />
    </context>
  </template>
  <template name="cmp_cmt" value="# ####################################################### $TEXT$ ####################################################### #" description="Complete comment" toReformat="false" toShortenFQNames="true">
    <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="print" value="print($SELECTION$)" description="print(***)" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>